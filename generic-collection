from prefect import task

@task
def process_dataset(dataset_id: int, normalize: bool = False):
    # ... your logic here ...
    return {"ok": True, "id": dataset_id, "normalized": normalize}

@task
def train_model(epochs: int = 10):
    # ... training logic ...
    return {"trained": True, "epochs": epochs}

# You can also export plain functions (not decorated) and they will be called directly
def helper(a: int, b: int) -> int:
    return a + b
